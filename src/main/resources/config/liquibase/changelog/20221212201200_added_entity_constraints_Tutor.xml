<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Tutor.
    -->
    <changeSet id="20221212201200-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="tutor_id"
                                 baseTableName="rel_tutor__courses"
                                 constraintName="fk_rel_tutor__courses__tutor_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tutor"/>

        <addForeignKeyConstraint baseColumnNames="courses_id"
                                 baseTableName="rel_tutor__courses"
                                 constraintName="fk_rel_tutor__courses__courses_id"
                                 referencedColumnNames="id"
                                 referencedTableName="course"/>
    </changeSet>
</databaseChangeLog>
