<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Student.
    -->
    <changeSet id="20221212201201-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="student_id"
                                 baseTableName="rel_student__courses"
                                 constraintName="fk_rel_student__courses__student_id"
                                 referencedColumnNames="id"
                                 referencedTableName="student"/>

        <addForeignKeyConstraint baseColumnNames="courses_id"
                                 baseTableName="rel_student__courses"
                                 constraintName="fk_rel_student__courses__courses_id"
                                 referencedColumnNames="id"
                                 referencedTableName="course"/>

        <addForeignKeyConstraint baseColumnNames="attendance_id"
                                 baseTableName="student"
                                 constraintName="fk_student__attendance_id"
                                 referencedColumnNames="id"
                                 referencedTableName="attendance"/>

        <addForeignKeyConstraint baseColumnNames="invoice_id"
                                 baseTableName="student"
                                 constraintName="fk_student__invoice_id"
                                 referencedColumnNames="id"
                                 referencedTableName="invoice"/>
    </changeSet>
</databaseChangeLog>
